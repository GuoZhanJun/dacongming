//嵌套指令
//#ifndef-#define-#endif三者的嵌套使用
//eg:
// 
// 
//在test.h中
//#ifndef __TEST_H__
//#define  __TEST_H__
//int Add(int x, int y);
//#endif
// 
// 
// 
//在test.c中
//#include "Add.c"
//int main()
//{
//	int a = 3;
//	int b = 5;
//	int ret=Add(a, b);
//	printf("%d\n", ret);
//	return 0;
//}

//在Add.c中
//#define _CRT_SECURE_NO_WARNINGS 1
//int Add(int x, int y)
//{
//	return x + y;
//}
