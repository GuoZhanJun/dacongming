//比较两个数的较大值
#include <stdio.h>//最初级
int get_max(int x, int y)
{
	if (x > y)
	return x;
	else
		return y;
}
int main()
{
	int a = 10;
	int b = 20;
	int max = get_max(a, b);
	printf("max= %d\n", max);
	max = get_max(100, 300);//框架写好以后可以直接调用
	printf("max= %d\n", max);
	return 0;
}
