---打印Hello World
#include <stdio.h>
int main()
{
	printf("Hello World!\n");
	return 0;
}

---打印两个数之和
#include <stdio.h>
int main()
{
	int a = 10;
	int b = 20;
	int sum = 0;
	sum = a + b;

	printf("sum=%d\n",sum);
	return 0;
}

---if 语句

#include <stdio.h>


int main()
{
	int score;
	printf("请输入0-100内的成绩：\n");
	scanf_s("%d",&score);
	if (score < 60)
	{
		printf("成绩不合格");
	}
	if (score >= 60)
	{
		printf("成绩合格");
	}
	return 0;
}

